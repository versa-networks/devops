{
  "group": [
    {
      "name": "{{bgp_group_name}}",
      "type": "external",
      "shutdown": "false",
      "family": {
        "inet": {
          "unicast": {
            "prefix-limit-control": {
              "action": "drop"
            }
          }
        }
      },
      "neighbor": [
        {
          "bgp-neighbor-ip": "{{concerto_tunnel_ip}}",
          "peer-as": "{{vcg_bgp_asn}}",
          "local-as": "{{sdwan_bgp_asn}}",
          "local-address": "{{sdwan_tunnel_ip}}",
          {% if bgp_password != "" %}
            "password": "{{bgp_password}}",
          {% endif %}
          "shutdown": "false",
          "prefix-limit": {
            "action": "drop"
          }
        }
      ],
      "prefix-limit": {
        "action": "drop"
      }
    }
  ]
}
